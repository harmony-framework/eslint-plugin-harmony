{
  "name": "eslint-plugin-harmony-boilerplate",
  "version": "1.0.0",
  "description": "Harmony Best Practices Plugin to keep developers alignment with best practices",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "",
  "files": [
    "lib/",
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "clear": "rimraf ./dist && rimraf ./lib",
    "clean:dist": "rimraf dist",
    "lint": "esw ./src --ext .ts,.tsx",
    "lint:watch": "esw ./src --ext .ts,.tsx -w --color",
    "lint:test": "esw ./test --ext .ts,.tsx",
    "lint-and-fix": "eslint ./src --ext .ts,.tsx --fix",
    "lite": "npm run clean:dist && poi --prod && lite-server",
    "build": "npm run lint && npm run clear && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "start": "poi --serve"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "@poi/plugin-typescript": "12.3.1",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "babel-loader": "8.0.4",
    "eslint-webpack-plugin": "2.4.1",
    "copy-webpack-plugin": "6.2.0",
    "cross-env": "5.2.0",
    "enzyme": "3.10.0",
    "eslint": "7.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-watch": "6.0.1",
    "mocha": "7.1.2",
    "npm-run-all": "latest",
    "poi": "12.10.3",
    "rimraf": "2.6.2",
    "sinon": "6.2.0",
    "ts-loader": "7.0.3",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.8.3",
    "lite-server": "^2.5.4"
  },
  "dependencies": {}
}
